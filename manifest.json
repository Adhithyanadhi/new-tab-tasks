{
  "manifest_version": 3,
  "name": "New tab tasks",
  "version": "1.0",
  "description": "Help managing task in a new tab chrome homepage",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "New tab"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
